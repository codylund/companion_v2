{
  "name": "web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "build": "npm-run-all build:*",
    "build:prod": "",
    "build:app": "ng build",
    "build:app:prod": "ng build --prod",
    "build:content": "cp -R ./src/main/content ./dist/content",
    "build:server": "tsc -p ./tsconfig.server.local.json",
    "clean:content": "rm -rf ./dist/content",
    "rebuild:content": "npm run clean:content && npm run build:content",
    "start": "npm run build && npm run start:server",
    "start:server:local": "set PORT=8080 && npm run start:server",
    "start:server": "node ./dist/server/index.js --port $PORT --static ./dist/www --content ./dist/content/posts"
  },
  "engines": {
    "node": "10.16.0"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.1.1",
    "@angular/common": "~8.1.1",
    "@angular/compiler": "~8.1.1",
    "@angular/core": "~8.1.1",
    "@angular/forms": "~8.1.1",
    "@angular/platform-browser": "~8.1.1",
    "@angular/platform-browser-dynamic": "~8.1.1",
    "@angular/router": "~8.1.1",
    "@types/command-line-args": "^5.0.0",
    "@types/cors": "^2.8.5",
    "@types/d3-geo": "^1.11.1",
    "@types/topojson": "^3.2.2",
    "body-parser": "^1.19.0",
    "command-line-args": "^5.1.1",
    "cors": "^2.8.5",
    "d3": "^5.9.7",
    "d3-geo": "^1.11.6",
    "express": "^4.17.1",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "ngx-gallery": "^5.10.0",
    "rxjs": "~6.4.0",
    "topojson": "^3.0.2",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.801.1",
    "@angular/cli": "~8.1.1",
    "@angular/compiler-cli": "~8.1.1",
    "@angular/language-service": "~8.1.1",
    "@types/d3": "^5.7.2",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^8.9.5",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "npm-run-all": "^4.1.5",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "^3.4.5"
  }
