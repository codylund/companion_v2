{
  "name": "web",
  "version": "0.0.0",
  "scripts": {
    "build": "npm run build:all:prod",
    "build:all:dev": "npm run build:app:dev && npm run build:content && npm run build:server",
    "build:all:prod": "npm run build:app:prod && npm run build:content && npm run build:server",
    "build:app:dev": "ng build",
    "build:app:prod": "ng build --prod",
    "build:content": "cp -R ./src/main/content ./dist/content",
    "build:flickr": "tsc -p ./tsconfig.flickr.json",
    "build:server": "tsc -p ./tsconfig.server.json",
    "clean:content": "rm -rf ./dist/content",
    "rebuild:content": "npm run clean:content && npm run build:content",
    "start": "npm run start:server:prod",
    "start:server": "node ./dist/server/index.js --port $PORT --static ./dist/www --content ./dist/content/places",
    "start:server:dev": "set PORT=8080 && npm run start:server",
    "start:server:prod": "npm run start:server -- --prod"
  },
  "engines": {
    "node": "10.16.0"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "github:angular/animations-builds",
    "@angular/cdk": "github:angular/cdk-builds",
    "@angular/common": "~8.1.1",
    "@angular/compiler": "~8.1.1",
    "@angular/core": "~8.1.1",
    "@angular/forms": "~8.1.1",
    "@angular/material": "github:angular/material2-builds",
    "@angular/platform-browser": "~8.1.1",
    "@angular/platform-browser-dynamic": "~8.1.1",
    "@angular/router": "~8.1.1",
    "@types/command-line-args": "^5.0.0",
    "@types/cors": "^2.8.5",
    "@types/d3-geo": "^1.11.1",
    "@types/express-sslify": "^1.2.0",
    "@types/topojson": "^3.2.2",
    "@types/xml2js": "^0.4.5",
    "body-parser": "^1.19.0",
    "command-line-args": "^5.1.1",
    "cors": "^2.8.5",
    "d3": "^5.9.7",
    "d3-geo": "^1.11.6",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-sslify": "^1.2.0",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "inquirer": "^7.0.0",
    "marked": "^0.7.0",
    "mongodb": "^3.3.2",
    "ngx-gallery": "^5.10.0",
    "ngx-markdown": "^8.1.0",
    "node-cron": "^2.0.3",
    "rxjs": "~6.4.0",
    "topojson": "^3.0.2",
    "tslib": "^1.9.0",
    "xml2js": "^0.4.22",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.801.1",
    "@angular/cli": "~8.1.1",
    "@angular/compiler-cli": "~8.1.1",
    "@angular/language-service": "~8.1.1",
    "@types/d3": "^5.7.2",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^8.9.5",
    "@types/node-cron": "^2.0.2",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
